<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DTO.UsuariosDTO" table="usuarios" schema="bd_neptuno2">
        <id name="nombre">
            <column name="nombre" sql-type="varchar(30)" length="30"/>
        </id>
        <property name="clave">
            <column name="clave" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>